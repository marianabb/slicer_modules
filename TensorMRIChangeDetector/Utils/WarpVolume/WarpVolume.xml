<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
    ITKMariana
  </category>
  <title>
    Warp Volume
  </title>
  <description>
    Applies a Deformation Field on a volume.
  </description>
  <version>0.1</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Mariana Bustamante</contributor>
<acknowledgements>
</acknowledgements>

  <parameters>
    <label>IO</label>
    <description>Input/output parameters</description>
    <image>
      <name>inVolume</name>
      <label>Input Volume</label>
      <channel>input</channel>
      <index>0</index>
      <description>Volume to be warped</description>
    </image>

    <image type="vector">
      <name>deformationField</name>
      <label>Deformation Field</label>
      <channel>input</channel>
      <index>1</index>
      <description>Deformation field</description>
    </image>

    <image>
      <name>warpedVolume</name>
      <label>Warped Volume</label>
      <channel>output</channel>
      <index>2</index>
      <description>Output warped volume</description>
    </image>


  </parameters>

</executable>
