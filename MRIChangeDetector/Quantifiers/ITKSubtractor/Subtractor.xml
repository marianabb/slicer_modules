<?xml version="1.0" encoding="utf-8"?>
<executable>
  <category>
  ITKMariana
  </category>
  <title>
  Subtraction test
  </title>
  <description>
    Subtracts two volumes. Output = Volume1 - Volume2.
  </description>
  <version>0.1</version>
  <documentation-url></documentation-url>
  <license></license>
  <contributor>Mariana Bustamante</contributor>
<acknowledgements>
</acknowledgements>

  <parameters>
    <label>IO</label>
    <description>Input/output parameters</description>
    <image>
      <name>inputVolume1</name>
      <label>Input Volume 1</label>
      <channel>input</channel>
      <index>0</index>
      <description>Volume1</description>
    </image>

    <image>
      <name>inputVolume2</name>
      <label>Input Volume 2</label>
      <channel>input</channel>
      <index>1</index>
      <description>Volume2</description>
    </image>

    <image type="label">
      <name>outputVolume</name>
      <label>Output Volume</label>
      <channel>output</channel>
      <index>2</index>
      <description>Output volume to keep the result.</description>
    </image>
  </parameters>

</executable>
